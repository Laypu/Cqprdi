@model List<SET_MENU>
@{
    Layout = "~/Areas/webadmin/Views/Shared/_Layout.cshtml";
}

<div class="page-bar">
    <ul class="page-breadcrumb">
        <li>
            <a href="@Url.Action("Index","Home")">Home</a>
            <i class="fa fa-circle"></i>
        </li>
        <li>
            <a href="#">模組元件</a>
            <i class="fa fa-circle"></i>
        </li>
    </ul>

</div>
<!--page bar end-->
<!--message start-->
<div class="title_01">選單程式模組管理</div>

<div class="mod_list">

    @foreach (var m in Model.OrderBy(t => t.M_Menu01))
    {
        <div class="col-lg-3 col-md-3 col-sm-4 col-xs-6">
            <div class="mt-widget-3">
                <div class="mt-head" onclick="location.href='@Url.Action("Index",m.M_Menu04)'">
                    <div class="mt-head-icon">
                        <i class="@m.M_Menu06"></i>
                    </div>
                    <div class="mt-head-desc">@m.M_Menu02</div>
                </div>
            </div>
        </div>
    }

</div>
