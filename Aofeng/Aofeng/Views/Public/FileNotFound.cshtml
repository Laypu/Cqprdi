    <script src="~/js/main_owl.js"></script>
    <!--owl.carousel end-->
</head>
<body>
    <!--header start-->
    @Html.Partial("~/Views/Shared/_Header.cshtml")
    <!--header end-->
    <!--content start-->
    <div id="content" class="page_content">
        <div class="container">

            <div class="page_top">
